{
  "name": "notes",
  "version": "1.1.2",
  "description": "Aplicativo de notas para desktop criado com Electron JS",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "win": "electron-packager . --icon=assets/logo/win/256x256.ico --out=packages --platform=win32 --arch=x64 --electron-version=12.0.4 --overwrite",
    "mac": "electron-packager . --icon=assets/logo/mac/16x16.icns --out=packages --platform=darwin --arch=x64 --electron-version=12.0.4 --overwrite",
    "linux": "electron-packager . --icon=assets/logo/png/256x256.png --out=packages --platform=linux --arch=x64 --electron-version=12.0.4 --overwrite"
  },
  "keywords": [],
  "author": "Jefferson",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.4"
  }
}
